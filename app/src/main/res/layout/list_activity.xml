<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="fill_parent"
    android:layout_height="wrap_content"
    android:layout_margin="0dp"
    android:background="@color/white"
    android:orientation="vertical"
    android:elevation="5dp">
    <TextView
        android:padding="5px"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:textAlignment="center"
        android:textStyle="bold"
        android:id="@+id/txtRecd"
        android:background="@color/blue"
        android:textColor="@color/black"/>

    <TextView
        android:text="DOC"
        android:id="@+id/teks1"
        android:layout_marginLeft="5dp"
        android:layout_marginRight="5dp"
        android:layout_below="@+id/txtRecd"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textColor="@color/black" />
    <TextView
        android:layout_marginLeft="65dp"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/txtRecd"
        android:layout_alignBaseline="@+id/teks1"
        android:id="@+id/txtDoc"
        android:textColor="@color/black"/>

    <RelativeLayout
        android:id="@+id/rl1"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/txtDoc">
    <TextView
        android:id="@+id/bgteks1"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"/>
    <TextView
        android:text="MORT"
        android:id="@+id/teks2"
        android:layout_marginLeft="5dp"
        android:layout_marginRight="5dp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textColor="@color/black" />
    <TextView
        android:layout_marginLeft="65dp"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignBaseline="@+id/teks2"
        android:id="@+id/txtMorta"
        android:textColor="@color/black"/>
    </RelativeLayout>

    <RelativeLayout
        android:id="@+id/rl2"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@id/rl1">
    <TextView
        android:id="@+id/bgteks2"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"/>
    <TextView
        android:text="ABW"
        android:id="@+id/teks3"
        android:layout_marginLeft="5dp"
        android:layout_marginRight="5dp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textColor="@color/black" />
    <TextView
        android:layout_marginLeft="65dp"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignBaseline="@+id/teks3"
        android:id="@+id/txtAvbw"
        android:textColor="@color/black"/>
    </RelativeLayout>

    <RelativeLayout
        android:id="@+id/rl3"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@id/rl2">
    <TextView
        android:id="@+id/bgteks3"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"/>
    <TextView
        android:text="FEEDC"
        android:id="@+id/teks4"
        android:layout_marginLeft="5dp"
        android:layout_marginRight="5dp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textColor="@color/black" />
    <TextView
        android:layout_marginLeft="65dp"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignBaseline="@+id/teks4"
        android:id="@+id/txtFedc"
        android:textColor="@color/black"/>
    </RelativeLayout>

    <ImageView
        android:id="@+id/garis"
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:layout_below="@+id/rl3"
        android:background="@color/black"/>
    <TextView
        android:text="SPPA"
        android:id="@+id/teks5"
        android:layout_marginLeft="5dp"
        android:layout_marginRight="5dp"
        android:layout_below="@+id/garis"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textColor="@color/black" />
    <TextView
        android:layout_marginLeft="65dp"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/garis"
        android:layout_alignBaseline="@+id/teks5"
        android:id="@+id/txtFnlq"
        android:textColor="@color/black"/>

    <TextView
        android:text="ABW"
        android:id="@+id/teks6"
        android:layout_marginLeft="5dp"
        android:layout_marginRight="5dp"
        android:layout_below="@+id/txtFnlq"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textColor="@color/black" />
    <TextView
        android:layout_marginLeft="65dp"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/txtFnlq"
        android:layout_alignBaseline="@+id/teks6"
        android:id="@+id/txtTobw"
        android:textColor="@color/black"/>

    <TextView
        android:text="REAL"
        android:id="@+id/teks7"
        android:layout_marginLeft="5dp"
        android:layout_marginRight="5dp"
        android:layout_below="@+id/txtTobw"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textColor="@color/black"/>
    <TextView
        android:layout_marginLeft="65dp"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/txtTobw"
        android:layout_alignBaseline="@+id/teks7"
        android:id="@+id/txtReasp"
        android:textColor="@color/black"/>
</RelativeLayout>